{
    "name": "enguerr/vmware-api",
    "description": "A PHP implementation for the VMWare API",
    "keywords": [
        "vmware",
	"vcenter",
        "api",
        "enguerr"
    ],
    "homepage": "https://github.com/enguerr/vmware-php",
    "license": "MIT",
    "require": {
        "php": "8.*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "7.8.*"
    },
    "autoload": {
        "psr-4": {
            "enguerr\\VMWare\\": "src"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
